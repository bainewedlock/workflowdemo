@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<h2>Workflows</h2>
<form method="post">
<table class="table table-bordered">
@foreach (var w in Model.Suspended)
{
    <tr>
    <td>@w.InstanceId</td>
    <td>@w.CreateTime</td>
    <td>@w.CompleteTime</td>
    <td>@w.Status</td>
    <td>@if (w.Status == 1)
        {
            <button asp-route-instanceid="@w.InstanceId"
            type="submit">resume</button> }
        </td>
    </tr>
}
</table>
</form>

<a href="demo">Start Demo Workflow</a>
